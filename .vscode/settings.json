{
    "workbench.iconTheme": "city-lights-icons-vsc",
    "editor.fontFamily": "'Fira Code Retina','Cascadia Code PL','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorBlinking": "blink",
    "editor.cursorStyle": "underline",
    "git.autofetch": true,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "editor.fontSize": 15.2,
    "go.toolsManagement.autoUpdate": true,
    "git.ignoreRebaseWarning": true,
    "files.autoSave": "onFocusChange",
    "settingsSync.ignoredSettings": [],
    "editor.fontLigatures": true,
    "editor.inlineSuggest.enabled": true,
    "explorer.confirmDelete": false,
    "editor.formatOnSave": true,
    "window.zoomLevel": 1,
    "terminal.integrated.fontFamily": "Fira Code Retina",
    "terminal.integrated.fontSize": 14.5,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "block",
}